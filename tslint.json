{
  "extends": ["tslint-react", "tslint:latest", "tslint-config-prettier"],
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "max-line-length": [true, 140],
    "quotemark": [true, "single", "jsx-double"],
    "semicolon": [false],
    "no-implicit-dependencies": [true, "dev"]
  }
}
